{
  "name": "soar",
  "version": "1.0.0",
  "private": true,
  "author": "Tre' Grisby <trewaters@hotmail.com>",
  "scripts": {
    "dev": "next",
    "concurrent": "concurrently \"mongod --config C:/data/config/mongod.conf\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .ts,.tsx --fix",
    "lint:errors": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:cache": "eslint . --ext .js,.jsx,.ts,.tsx --quiet --cache --cache-location .eslintcache",
    "lint:scope": "eslint --ext .js,.jsx,.ts,.tsx",
    "guard:navigator": "node scripts/guards/check-no-legacy-navigator-paths.mjs",
    "lint:sequences": "eslint app/sequences --ext .tsx --quiet --cache --cache-location .eslintcache",
    "test": "jest --detectOpenHandles --forceExit",
    "test:single": "jest --detectOpenHandles --forceExit --testNamePattern=",
    "test:coverage": "jest --coverage",
    "test:bail": "jest --onlyFailures --bail",
    "test:failures": "jest --detectOpenHandles --forceExit --verbose --no-coverage --reporters=default",
    "test:minimal": "jest --detectOpenHandles --forceExit --no-coverage --passWithNoTests --silent",
    "test:clean": "jest --detectOpenHandles --forceExit --no-coverage --verbose --no-cache --clearCache",
    "cover": "jest --coverage",
    "pretty": "prettier --l .",
    "pretty-fix": "prettier --write . && prettier --list-different .",
    "postinstall": "prisma generate && prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "prisma db push",
    "mongo": "mongod --config C:/data/config/mongod.conf",
    "docs": "typedoc --options typedoc.web.json",
    "docs:web": "typedoc --options typedoc.web.json",
    "docs:md": "typedoc --options typedoc.json",
    "import:asana-activity": "ts-node scripts/migrations/import-asana-activity.ts",
    "import:series-activity": "ts-node scripts/migrations/import-series-activity.ts",
    "import:sequence-activity": "ts-node scripts/migrations/import-sequence-activity.ts",
    "import:all-activities": "npm run import:asana-activity && npm run import:series-activity && npm run import:sequence-activity",
    "db:sync": "tsx scripts/syncDb/sync-db-from-prod.ts",
    "db:backup-prod": "tsx scripts/syncDb/backup-prod-db.ts",
    "db:restore-backup": "tsx scripts/syncDb/restore-from-backup.ts",
    "db:inspect-prod": "tsx scripts/syncDb/inspect-prod-db.ts",
    "script:set-admin": "ts-node scripts/set-admin-role.ts",
    "script:migrate-public": "ts-node scripts/migrate-data-ownership.ts",
    "script:verify-ownership": "ts-node scripts/verify-ownership.ts",
    "script:fix-null-creators": "ts-node scripts/fix-null-creators-mongodb.ts",
    "script:complete-alpha-migration": "ts-node scripts/complete-alpha-migration.ts",
    "script:fix-null-roles": "ts-node scripts/fix-null-roles.ts",
    "script:repair-series-poses": "ts-node scripts/migrations/repair-series-pose-references.ts",
    "script:repair-series-poses:apply": "ts-node scripts/migrations/repair-series-pose-references.ts --apply",
    "script:repair-series-poses:prod": "ts-node scripts/migrations/repair-series-pose-references.ts --production --i-understand-production",
    "script:repair-series-poses:prod:apply": "ts-node scripts/migrations/repair-series-pose-references.ts --production --apply --i-understand-production"
  },
  "dependencies": {
    "@auth/core": "^0.37.3",
    "@auth/mongodb-adapter": "^3.4.1",
    "@auth/prisma-adapter": "^2.4.1",
    "@aws-sdk/credential-providers": "^3.693.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mongodb-js/zstd": "^1.2.2",
    "@mui/icons-material": "^ 6.1.7",
    "@mui/lab": "^6.0.0-beta.15",
    "@mui/material": "^6.1.7",
    "@next/third-parties": "^15.5.4",
    "@prisma/client": "^6.19",
    "@react-google-maps/api": "^2.20.7",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.7.2",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.6.1",
    "form-data": "^4.0.3",
    "gcp-metadata": "^5.3.0",
    "jest-axe": "^10.0.0",
    "jwt-decode": "^4.0.0",
    "luxon": "^3.4.4",
    "mongodb": "^6.21.0",
    "next": "15.5.9",
    "next-auth": "^5.0.0-beta.19",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use-precision-timer": "^3.5.6",
    "recharts": "^3.3.0",
    "resend": "^6.1.0",
    "snappy": "^7.2.2",
    "socks": "^2.8.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.0",
    "@jest/globals": "^29.7.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^5.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.1",
    "@types/node": "^20.19.31",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/nft": "^0.27.3",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.36.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.2.1",
    "prisma": "^6.19",
    "semantic-release": "^23.0.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.9.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
