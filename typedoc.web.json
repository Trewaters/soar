{
  "entryPoints": ["app", "types", "lib"],
  "entryPointStrategy": "expand",
  "out": "public/docs",
  "tsconfig": "tsconfig.typedoc.json",
  "exclude": ["**/__tests__/**", "node_modules"],
  "excludePrivate": true,
  "hideGenerator": true,
  "cleanOutputDir": true,
  "blockTags": [
    "@param",
    "@returns",
    "@return",
    "@throws",
    "@category",
    "@deprecated",
    "@example",
    "@group",
    "@inheritDoc",
    "@label",
    "@link",
    "@remarks",
    "@see",
    "@since",
    "@template",
    "@typeParam",
    "@defaultValue",
    "@default",
    "@property",
    "@module",
    "@hidden",
    "@ignore",
    "@internal",
    "@event",
    "@component"
  ],
  "disableGit": true,
  "disableSources": true
}
